set postmaster "ryan"
set bouncemail
set no spambounce
set softbounce
set properties ""
poll imap.gmail.com with proto IMAP
       user '' there with password '' is 'ryan' here options keep ssl mda "procmail .procmailrc.gmail"

poll neo.tamu.edu with proto IMAP
       user '' there with password ''  options keep ssl mda "procmail .procmailrc.neo"
